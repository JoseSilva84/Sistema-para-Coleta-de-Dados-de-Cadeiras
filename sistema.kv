            height: self.texture_size[1]
            pos_hint: {"center_x": 0.7}
            #halign: "letf"
            bold: True

<Backup>:
    id: backup
    name: "backup"
    MDBoxLayout:
        orientation: "vertical"
        md_bg_color: 1.00, 1.00, 0.80, 1
    MDTopAppBar:
        title: "SISTEMA ART & FIBRAS CADEIRAS"
        right_action_items: [["calendar", lambda x: app.show_data_picker()], ["menu", lambda x: app.open_menu(x)], ["close", lambda x: app.sair_programa()]]
        left_action_items: [["home", lambda x: app.change_screen("principal")], ["calculator", lambda x: app.abrir_calculadora()]]
        elevation: 2
        pos_hint: {"top": 1}

    MDBoxLayout:
        orientation: 'vertical'
        MDBoxLayout:
            orientation: 'horizontal'

            MDProgressBar:
                id: progress
                value: 0
                max: 100
                pos_hint: {"center_x": .5}
                size_hint_x: 0.05
                size_hint_y: 0.05
                # height: 1
                md_bg_color: app.theme_cls.primary_color
                color: 0, 1, 0, 1

        MDRaisedButton:
            id: backup_botao
            text: "[b]Iniciar Backup[/b]"
            markup: True
            pos_hint: {"center_x": .5}
            on_release: app.iniciar_backup()
        MDRaisedButton:
            id: backup_restaurar
            text: "[b]Restaurar Backup[/b]"
            markup: True
            pos_hint: {"center_x": .5}
            on_release: app.realizar_restauracao()
