<Login>:
    id: logininicial
    name: "logininicial"
    MDBoxLayout:
        orientation: 'horizontal'
        md_bg_color: 1.00, 1.00, 0.80, 1
        spacing: dp(30)  # Espaçamento entre os botões e a imagem
        padding: dp(40)  # Padding para dar um espaço interno no layout

        Image:
            source: "entrar.png"
            size_hint_y: None  # Desativa o ajuste automático de altura
            size_hint_x: None  # Desativa o ajuste automático de largura
            width: dp(550)  # Defina a largura da imagem
            height: dp(550)  # Defina a altura da imagem
            allow_stretch: True  # Permite que a imagem se expanda para o tamanho definido

        MDBoxLayout:
            orientation: 'vertical'
            size_hint_x: 0.4  # Ajuste para os botões ocuparem o restante do espaço
            size_hint_y: None  # Permite o ajuste da altura dos botões
            height: self.minimum_height  # Ajusta a altura conforme o conteúdo
            pos_hint: {"center_y": 0.7}  # Centraliza os botões no eixo vertical
            spacing: dp(10)  # Espaçamento entre os botões e a imagem
            MDLabel:
                text: "SEJA BEM-VINDO AO SISTEMA"
                height: self.texture_size[1]
                font_name: "assets/fonts/cambria.ttc"
                theme_text_color: "Custom"
                text_color: 0.21, 0.35, 0.38, 1
                size_hint_y: None
                pos_hint: {"center_x": -0.1, "center_y": 0.5}
                halign: "center"
                font_style: 'H3'
                bold: True

            MDCard:
                elevation: 1
                radius: [10,]
                size_hint: None, None
                height: dp(100)
                width: dp(300)
                pos_hint: {"center_x": -0.1, "center_y": 0.4}  # Levanta o MDCard um pouco mais
                md_bg_color: [0.21, 0.35, 0.38, 0.8]
                spacing: dp(10)  # Espaçamento entre os botões e a imagem
                padding: dp(40)

                MDRaisedButton:
                    id: entrar
                    text: "[b]ENTRAR[/b]"
                    markup: True
                    size_hint: None, None
                    size: dp(120), dp(48)
                    md_bg_color: app.normal_color
                    pos_hint: {"center_x": -0.1, "center_y": 0.4}
                    on_release:
                        root.manager.transition.direction = 'right'
                        app.root.current = "loginsenha"

                MDRaisedButton:
                    id: cadastrar
                    text: "[b]CADASTRAR-SE[/b]"
                    markup: True
                    size_hint: None, None
                    size: dp(120), dp(48)
                    pos_hint: {"center_x": -0.1, "center_y": 0.4}
                    on_release:
                        root.manager.transition.direction = 'up'
                        app.root.current = "logincadastro"
<Loginsenha>:
    id: loginsenha
    name: "loginsenha"
    MDBoxLayout:
        orientation: 'vertical'
        md_bg_color: 1.00, 1.00, 0.80, 1
        pos_hint: {"center_x": 0.5, "center_y": 0.5}  # Levanta o layout um pouco
        padding: dp(10)  # Adiciona padding para ajustar o espaço
        spacing: dp(5)  # Espaçamento entre os elementos
    MDFloatLayout:
        # MDIconButton:
        #     id: setavoltar
        #     icon: "arrow-left"
        #     user_font_size: "32sp"  # Tamanho da seta
        #     pos_hint: {"center_x": 0.05, "top": 1}  # Posição na parte superior esquerda
        #     on_release: app.root.current = "logininicial"  # Ação ao clicar na seta
        #     tooltip_text: "Voltar"  # Texto exibido ao passar o mouse

        Image:
            source: "skinlogin.png"
            pos_hint: {"center_x": 0.5, "center_y": 0.7}
            size_hint: .2, .2

        MDCard:
            elevation: 1
            radius: [10,]
            size_hint: None, None
            height: dp(250)
            width: dp(250)
            pos_hint: {"center_x": 0.5, "center_y": 0.4}  # Levanta o MDCard um pouco mais
            md_bg_color: [1.00, 0.93, 0.74]

            MDBoxLayout:
                orientation: 'vertical'
                padding: dp(20)
                spacing: dp(10)
                pos_hint: {"center_x": 0.5, "center_y": 0.5}

                MDLabel:
                    text: "LOGIN"
                    halign: "center"
                    font_style: 'H6'
                    bold: True

                MDTextField:
                    id: usuario
                    font_size: 18
                    size_hint_x: 0.9
                    hint_text: 'Usuário'
                    pos_hint: {'center_x': 0.5}
                    icon_right: 'mdiAccount'
                    icon_right_color: app.theme_cls.primary_color
                    helper_text: 'Digite o nome de usuário'
                    helper_text_mode: "on_focus"
                    helper_font_size: 0.19
                    multiline: False

                MDTextField:
                    id: senha
                    font_size: 18
                    size_hint_x: 0.9
                    hint_text: 'Senha'
                    pos_hint: {'center_x': 0.5}
                    password: app.is_password_hidden
                    icon_right: "eye-off" if app.is_password_hidden else "eye" 
                    icon_right_color: app.theme_cls.primary_color
                    on_icon_right: app.toggle_password_visibility()  # Ação ao clicar no ícone
                    helper_text: 'Digite a senha'
                    helper_text_mode: "on_focus"
                    helper_font_size: 0.19
                    multiline: False

                MDRaisedButton:
                    id: cadastrar2
                    text: "Entrar"
                    size_hint: None, None
                    size: dp(200), dp(40)
                    pos_hint: {"center_x": 0.5}
                    on_release:
                        root.manager.transition.direction = 'up'
                        app.loginentrada()
