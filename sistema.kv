<SecondScreen>:
    id: segunda
    name: "segunda"
    MDBoxLayout:
        orientation: "vertical"
        md_bg_color: 1.00, 1.00, 0.80, 1
        MDTopAppBar:
            title: "SISTEMA ART & FIBRAS CADEIRAS"
            right_action_items: [["calendar", lambda x: app.show_data_picker()], ["menu", lambda x: app.open_menu(x)], ["close", lambda x: app.sair_programa()]]
            left_action_items: [["home", lambda x: app.change_screen("principal")], ["calculator", lambda x: app.abrir_calculadora()]]
            elevation: 2
        MDFloatLayout:
            Image:
                source: "enrolamento.png"
                pos_hint: {"center_x": 0.5, "center_y": 0.7}
                size_hint: .7, .7
                size: 200, 200

        BoxLayout:
            id: box_enrolador
            orientation: "vertical"
            height: dp(56)
            padding: dp(20)
            spacing: dp(10)

            MDCard:
                elevation: 1  # Defina a elevação aqui
                radius: [10,]  # Arredondar os cantos, opcional
                size_hint: None, None
                height: dp(25)
                width: dp(300)
                pos_hint: {"center_x": 0.5, "center_y": 0.1}
                md_bg_color: [0.99, 0.75, 0.03]  # Cor de fundo do Card

                MDLabel:
                    text: "PRODUÇÃO DOS ENROLADORES"
                    halign: "center"
                    font_style: 'H6'
                    bold: True

            MDTextField:
                id: enrolador_input
                font_size: 18
                size_hint_x: 0.5
                hint_text: 'Enrolador:'.capitalize()
                pos_hint: {'center_x':0.5, 'center_y':0.6}
                icon_right: 'mdiAccount'
                icon_right_color: app.theme_cls.primary_color
                width:200
                helper_text: 'Digite o nome do enrolador'
                helper_text_mode: "on_focus"
                helper_font_size: 0.19
                multiline: False
            MDTextField:
                id: list_objeto
                font_size: 18
                size_hint_x: 0.5
                hint_text: 'Objeto:'.capitalize()
                pos_hint: {'center_x':0.5, 'center_y':0.6}
                icon_right: 'mdiAccount'
                icon_right_color: app.theme_cls.primary_color
                width:200
                helper_text: 'Digite o Objeto'
                helper_text_mode: "on_focus"
                helper_font_size: 0.19
                multiline: False
            MDTextField:
                id: qtde_input
                font_size: 18
                size_hint_x: 0.5
                hint_text: 'Quantidade:'.capitalize()
                input_filter: 'int'
                pos_hint: {'center_x':0.5, 'center_y':0.6}
                icon_right: 'mdiAccount'
                icon_right_color: app.theme_cls.primary_color
                width:200
                helper_text: 'Digite a quantidade'
                helper_text_mode: "on_focus"
                helper_font_size: 0.19
                multiline: False
            MDTextField:
                id: valor_unitario
                font_size: 18
                size_hint_x: 0.5
                hint_text: 'Valor:'.capitalize()
                input_filter: 'float'
                pos_hint: {'center_x':0.5, 'center_y':0.6}
                icon_right: 'mdiAccount'
                icon_right_color: app.theme_cls.primary_color
                width:200
                helper_text: 'Digite o valor do objeto'
                helper_text_mode: "on_focus"
                helper_font_size: 0.19
                multiline: False
            MDRaisedButton:
                id: buttonenrolador
                text: "Registrar os dados"
                pos_hint: {"center_x": 0.5}
                on_release: app.verificar_campos()

